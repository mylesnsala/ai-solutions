{
  "storage": {
    "rules": "storage.rules",
    "cors": {
      "origin": [
        "*"
      ],
      "method": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "OPTIONS",
        "PATCH",
        "HEAD",
        "CONNECT",
        "TRACE"
      ],
      "maxAgeSeconds": 3600
    }
  },
  "functions": {
    "source": "functions"
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  }
}
